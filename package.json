{
  "name": "cognova",
  "version": "1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.js --fix",
    "env": "dotenv -e .env.local",
    "postinstall": "prisma generate && prisma db push",
    "prisma:migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:db:seed": "dotenv -e .env.local prisma db seed",
    "prisma:db:push": "dotenv -e .env.local prisma db push",
    "prisma:db:pull": "dotenv -e .env.local prisma db pull",
    "prisma:studio": "dotenv -e .env.local prisma studio --browser none",
    "prisma:generate": "dotenv -e .env.local prisma generate",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "fastapi:generate": "openapi -i 'http://127.0.0.1:8000/openapi.json' -o ./fastapi  --name ApiClient"
  },
  "dependencies": {
