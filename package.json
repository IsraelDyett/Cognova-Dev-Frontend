{
	"name": "cognova",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
		"postinstall": "prisma generate && prisma db push && prisma db seed",
		"prisma:dev": "dotenv -e .env.local prisma migrate dev",
		"prisma:seed": "dotenv -e .env.local prisma db seed",
		"prisma:db:push": "dotenv -e .env.local prisma db push",
		"prisma:db:pull": "dotenv -e .env.local prisma db pull",
		"prisma:studio": "dotenv -e .env.local prisma studio --browser none",
		"prisma:generate": "dotenv -e .env.local prisma generate"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.9.0",
		"@prisma/client": "^5.21.1",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-slot": "^1.1.0",
		"appwrite": "^16.0.2",
		"bcrypt": "^5.1.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"framer-motion": "~11.1.1",
		"lucide-react": "^0.453.0",
		"ms": "^2.1.3",
		"next": "14.2.1",
		"next-auth": "^5.0.0-beta.25",
		"next-themes": "^0.2.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"slugify": "^1.6.6",
		"sonner": "^1.5.0",
		"tailwind-merge": "^2.5.4",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/ms": "^0.7.34",
		"@types/node": "^20.5.7",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "7.2.0",
		"@typescript-eslint/parser": "7.2.0",
		"autoprefixer": "10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-next": "14.2.1",
		"postcss": "8.4.38",
		"prisma": "^5.21.1",
		"tailwind-variants": "0.1.20",
		"tailwindcss": "3.4.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.4"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	}
}
