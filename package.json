{
	"name": "cognova",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
		"postinstall": "prisma generate && prisma db push && prisma db seed",
		"prisma:dev": "dotenv -e .env.local prisma migrate dev",
		"prisma:seed": "dotenv -e .env.local prisma db seed",
		"prisma:db:push": "dotenv -e .env.local prisma db push",
		"prisma:db:pull": "dotenv -e .env.local prisma db pull",
		"prisma:studio": "dotenv -e .env.local prisma studio --browser none",
		"prisma:generate": "dotenv -e .env.local prisma generate"
	},
	"dependencies": {
		"@nextui-org/button": "2.0.38",
		"@nextui-org/card": "^2.0.34",
		"@nextui-org/checkbox": "^2.1.5",
		"@nextui-org/input": "2.2.5",
		"@nextui-org/link": "2.0.35",
		"@nextui-org/snippet": "2.0.43",
		"@nextui-org/switch": "2.0.34",
		"@nextui-org/system": "^2.2.6",
		"@nextui-org/theme": "^2.2.11",
		"@prisma/client": "^5.21.1",
		"@react-aria/ssr": "3.9.4",
		"@react-aria/visually-hidden": "3.8.12",
		"@types/bcrypt": "^5.0.2",
		"bcrypt": "^5.1.1",
		"clsx": "2.1.1",
		"framer-motion": "~11.1.1",
		"next": "14.2.4",
		"next-themes": "^0.2.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"slugify": "^1.6.6",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@types/node": "^20.5.7",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "7.2.0",
		"@typescript-eslint/parser": "7.2.0",
		"autoprefixer": "10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-next": "14.2.1",
		"postcss": "8.4.38",
		"prisma": "^5.21.1",
		"tailwind-variants": "0.1.20",
		"tailwindcss": "3.4.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.4"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	}
}
